{
  "name": "friendsoft",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --report-unused-disable-directives --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "dev:ssr": "tsx src/ssr/server.ts",
    "build:ssr": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/ssr/entry-server.tsx --outDir dist/server/entry --ssrManifest --emptyOutDir false && vite build --ssr src/ssr/start-server.ts --outDir dist/server/static --emptyOutDir false && cross-env VITE_SSR_RUNTIME=1 vite build --ssr src/ssr/server.ts --outDir dist/server/runtime --emptyOutDir false",
    "preview:ssr": "cross-env NODE_ENV=production node dist/server/runtime/assets/js/server-70b71c83.js",
    "clean": "rimraf dist",
    "build:clean": "npm run clean && npm run build:ssr",
    "lighthouse": "lhci autorun",
    "lighthouse:collect": "lhci collect",
    "lighthouse:assert": "lhci assert",
    "lighthouse:upload": "lhci upload",
    "performance:check": "npm run build:ssr && npm run lighthouse",
    "performance:local": "node scripts/performance-check.js",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "optimize:imports": "node scripts/optimize-imports.js",
    "docker:build": "docker build -t mi-pagina .",
    "docker:run": "docker run -d -p 3013:3000 --name mi-pagina-container mi-pagina",
    "docker:run:dev": "docker run -it -p 3013:3000 --name mi-pagina-dev mi-pagina",
    "docker:stop": "docker stop mi-pagina-container",
    "docker:remove": "docker rm mi-pagina-container",
    "docker:logs": "docker logs mi-pagina-container",
    "docker:logs:follow": "docker logs -f mi-pagina-container",
    "docker:clean": "docker stop mi-pagina-container && docker rm mi-pagina-container",
    "docker:rebuild": "npm run docker:clean && npm run docker:build && npm run docker:run",
    "docker:shell": "docker exec -it mi-pagina-container sh",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "docker:prune": "docker system prune -f",
    "deploy:prod": "npm run build:ssr && npm run docker:build"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@types/express": "^5.0.3",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^24.3.0",
    "compression": "^1.8.1",
    "express": "^5.1.0",
    "framer-motion": "^12.23.12",
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^13.2.2",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.16.0",
    "sirv": "^3.0.1",
    "tsx": "^4.20.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@lhci/cli": "^0.15.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@types/compression": "^1.8.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "cross-env": "^10.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "globals": "^16.3.0",
    "postcss": "^8.4.30",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.3.3",
    "terser": "^5.43.1",
    "typescript": "^5.0.2",
    "typescript-eslint": "^8.38.0",
    "vite": "^4.4.5"
  }
}
